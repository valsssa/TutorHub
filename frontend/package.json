{
  "name": "tutor-booking-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:tests": "tsc --project tsconfig.test.json --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
